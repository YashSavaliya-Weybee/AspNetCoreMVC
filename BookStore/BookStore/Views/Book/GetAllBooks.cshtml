@{
    ViewData["Title"] = "All Books";
}


<div class="container">
    <h1 class="display-4">All book list</h1>
    <div class="row">

        @foreach (var book in Model)
        {
            <div class="col-md-4">
                <partial name="_bookThumbnail" model="@book" />
            </div>
        }
    </div>
</div>
